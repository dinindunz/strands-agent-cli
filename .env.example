# Agent and Cognee LLM Configuration
# Use openai/ prefix for litellm and custom model name

LLM_PROVIDER=openai
LLM_MODEL=openai/global-claude-sonnet-4.5-claude-code
LLM_ENDPOINT=https://ai.xxxxxxx.com.au/v1
LLM_API_KEY=sk-xxxxxxx

# Embedding configuration
# Using large model (3072 dimensions) to match cognee's vector database schema
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=openai/au-text-embedding-3-large
EMBEDDING_ENDPOINT=https://ai.xxxxxxx.com.au/v1
EMBEDDING_API_KEY=sk-xxxxxxx

# Server Configuration
AGENT_PORT=8888
GRAPH_PORT=8889

# Strands settings
STRANDS_NON_INTERACTIVE=true
BYPASS_TOOL_CONSENT=true